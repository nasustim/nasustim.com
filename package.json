  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "next": "^12.1.6",
    "next-sitemap": "^3.0.5",
    "normalize.css": "^8.0.1",
    "postcss-scss": "^4.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "remark-parse": "^10.0.1",
    "sass": "^1.52.3",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "npm-check-updates": "^13.1.4",
    "prettier": "^2.6.2",
    "stylelint": "^14.9.1",
    "stylelint-config-property-sort-order-smacss": "^9.0.0"
  },
  "homepage": "https://github.com/nasustim/nasustim.com#readme",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nasustim/nasustim.com.git"
  },
  "scripts": {
    "format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "test": "echo \"please write test and execute\" | exit 0",
    "start": "next dev",
    "build": "next build",
    "sitemap": "next-sitemap --config ./sitemap.config.js",
    "serve": "next start",
    "pre-commit": "npm run format",
    "pre-push": "npm run test",
    "export": "next export",
    "stylelint": "stylelint --fix \"**/*.scss\" \"!**/node_modules/**\"",
    "update": "ncu",
    "upgrade": "ncu -u && npm install",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format"
    ],
    "*.{css,scss}": [
      "npm run stylelint"
    ]
  }
}
