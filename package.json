{
  "name": "nasustim.com",
  "private": true,
  "version": "0.0.1",
  "author": "nasustim",
  "bugs": {
    "url": "https://github.com/nasustim/nasustim.com/issues"
  },
  "dependencies": {
    "@acab/reset.css": "0.5.3",
    "@types/node": "18.16.10",
    "@types/react": "18.2.6",
    "next": "13.4.2",
    "next-sitemap": "4.0.9",
    "postcss-scss": "4.0.6",
    "react": "18.2.0",
    "react-icons": "4.8.0",
    "three": "0.152.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/three": "0.152.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "sass": "1.62.1",
    "stylelint": "15.6.2",
    "stylelint-config-property-sort-order-smacss": "9.1.0",
    "stylelint-config-standard-scss": "9.0.0",
    "stylelint-scss": "5.0.0",
    "stylelint-selector-bem-pattern": "2.1.1",
    "typescript-plugin-css-modules": "5.0.1"
  },
  "homepage": "https://github.com/nasustim/nasustim.com#readme",
  "main": "src/pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nasustim/nasustim.com.git"
  },
  "scripts": {
    "ts:format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "ts:check": "prettier -c \"src/**/*.{ts,tsx}\"",
    "test": "echo \"please write test and execute\" | exit 0",
    "start": "next dev",
    "build": "next build",
    "sitemap": "next-sitemap --config ./sitemap.config.js",
    "serve": "next start",
    "pre-commit": "npm run format",
    "stylelint:fix": "stylelint --fix \"src/**/*.{scss,css}\" \"!**/node_modules/**\"",
    "stylelint:check": "stylelint \"src/**/*.{scss,css}\" \"!**/node_modules/**\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run ts:format"
    ],
    "*.{css,scss}": [
      "npm run stylelint:fix"
    ]
  }
}
