{
  "name": "nasustim.com",
  "private": true,
  "version": "0.0.1",
  "author": "nasustim",
  "bugs": {
    "url": "https://github.com/nasustim/nasustim.com/issues"
  },
  "dependencies": {
    "@acab/reset.css": "^0.5.3",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "next": "13.0.6",
    "next-sitemap": "3.1.32",
    "postcss-scss": "^4.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "three": "0.147.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/three": "0.146.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.4",
    "prettier": "2.8.1",
    "sass": "^1.55.0",
    "stylelint": "14.16.0",
    "stylelint-config-property-sort-order-smacss": "9.0.0",
    "stylelint-config-standard-scss": "^6.1.0"
  },
  "homepage": "https://github.com/nasustim/nasustim.com#readme",
  "main": "src/pages/index.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nasustim/nasustim.com.git"
  },
  "scripts": {
    "ts:format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "ts:check": "prettier -c \"src/**/*.{ts,tsx}\"",
    "test": "echo \"please write test and execute\" | exit 0",
    "start": "next dev",
    "build": "next build",
    "sitemap": "next-sitemap --config ./sitemap.config.js",
    "serve": "next start",
    "pre-commit": "npm run format",
    "export": "next export",
    "stylelint:fix": "stylelint --fix \"src/**/*.{scss,css}\" \"!**/node_modules/**\"",
    "stylelint:check": "stylelint \"src/**/*.{scss,css}\" \"!**/node_modules/**\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run ts:format"
    ],
    "*.{css,scss}": [
      "npm run stylelint:fix"
    ]
  }
}
